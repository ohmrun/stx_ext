package stx.core.head;

import stx.core.type.Clause in ClauseT;
class Clauses{
  static public var _ = new stx.core.body.Clauses();
  static public function lift<Subject,Verb>(clause:ClauseT<Subject,Verb>):Clause<Subject,Verb>{
    return new Clause(clause);
  }
  static public function make<Subject,Verb>(brand:Subject,media:Verb):Clause<Subject,Verb>{
    return lift({ brand : brand, media : media });
  }
}